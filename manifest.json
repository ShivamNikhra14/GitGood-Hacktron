{
  "manifest_version": 3,
  "name": "AI Quiz Generator",
  "version": "1.0",
  "description": "Generate quizzes using AI ono Google Forms.",
  "permissions": ["scripting", "activeTab"],
  "action":{
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ]
}